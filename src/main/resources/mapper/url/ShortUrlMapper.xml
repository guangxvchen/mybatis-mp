<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mmp.mp.url.mapper.ShortUrlMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mmp.mp.url.entity.ShortUrl">
        <id column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="md5" property="md5"/>
        <result column="url_key" property="urlKey"/>
        <result column="create_time" property="createTime"/>
        <result column="url_value" property="urlValue"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, md5, url_key, create_time, url_value
    </sql>

</mapper>
